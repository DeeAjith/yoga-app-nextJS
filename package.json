{
    "name": "yoga-poses-app",
    "version": "1.0.0",
    "description": "Yoga Poses App",
    "main": "index.js",
    "scripts": {
      "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
      "start-backend": "concurrently \"cd backend && npm start\"",
      "start-frontend": "concurrently \"cd frontend && npm start\"",
      "build": "concurrently \"npm run build-frontend\"",
      "build-frontend": "concurrently \"cd frontend && npm run build\"",
      "dev": "concurrently \"npm run dev-backend\" \"npm run dev-frontend\"",
      "dev-backend": "concurrently \"cd backend && npm run dev\"",
      "dev-frontend": "concurrently \"cd frontend && npm run dev\"",
      "lint": "concurrently \"npm run lint-backend\" \"npm run lint-frontend\"",
      "lint-backend": "concurrently \"cd backend && npm run lint\"",
      "lint-frontend": "concurrently \"cd frontend && npm run lint\"",
      "test": "concurrently \"npm run test-backend\" \"npm run test-frontend\"",
      "test-backend": "concurrently \"cd backend && npm test\"",
      "test-frontend": "concurrently \"cd frontend && npm test\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {},
    "devDependencies": {
      "concurrently": "^7.0.0"
    }
  }
  